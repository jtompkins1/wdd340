<header id="top-header">
  <span class="siteName">
    <a href="/" title="Return to home page">CSE Motors</a>
  </span>
  <div id="tools">
    <% if (loggedIn) { %>
      <a href="/account/" title="Go to account management">Welcome, <%= userName %> </a>
      <a href="/account/favorites" title="Go to favorites"> | Favorites</a>
      <a href="/account/logout" title="Click to log out">| Logout</a>
    <% } else { %>
      <a href="/account/login" title="Click to log in">My Account</a>
    <% } %>
  </div>
</header>